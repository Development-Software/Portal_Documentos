        <add assembly="K4os.Compression.LZ4, Version=1.1.11.0, Culture=neutral, PublicKeyToken=2186FA9121EF231D" />
        <add assembly="K4os.Hash.xxHash, Version=1.0.6.0, Culture=neutral, PublicKeyToken=32CD54395057CEC3" />
