        <add assembly="MySql.Data, Version=8.0.23.0, Culture=neutral, PublicKeyToken=C5687FC88969C44D" />
        <add assembly="Ubiety.Dns.Core, Version=2.2.1.0, Culture=neutral, PublicKeyToken=C5687FC88969C44D" />
